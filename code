
#include <iostream>
using namespace std;

int sol(int a);

int main() {
	int n;
	cin>>n;
	cout<<"1 ";
	for(int i =2;i <= n;i++){
		
		cout<<sol(i)<<" ";

	}
	
	

}
int sol(int a){
	int ans = 1;
	if(a > 1){
	    for(int i = 2;i<=a;i++){
		ans = (ans + (i-1)*sol(i-1))%1000000007;
	    }
	
	}
	return ans;
	
}